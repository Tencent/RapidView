{
	"name": "lsp-sample",
	"description": "Example implementation of a language server in node.",
	"version": "0.0.1",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"engines": {
		"node": "*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"dependencies": {
		"vscode-languageserver": "^4.1.2"
	},
	"devDependencies": {
		"typescript": "^2.5.3",
		"@types/node": "^6.0.101"
	},
	"scripts": {
		"installServer": "installServerIntoExtension ../rapidstudio ./package.json ./tsconfig.json",
		"compile": "installServerIntoExtension ../rapidstudio ./package.json ./tsconfig.json && tsc -p .",
		"watch": "installServerIntoExtension ../rapidstudio ./package.json ./tsconfig.json && tsc -w -p ."
	}
}
